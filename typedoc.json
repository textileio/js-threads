{
  "readme": "./README.md",
  "name": "@textile/threads",
  "mode": "modules",
  "out": "docs",
  "exclude": [
    "**/*.spec.ts",
    "**/node_modules/**"
  ],
  "lernaExclude": [
    "@textile/context",
    "@textile/multiaddr",
    "@textile/threads-core",
    "@textile/threads-encoding",
    "@textile/threads-network",
    "@textile/threads-network-client",
    "@textile/threads-store"
  ],
  "gitRevision": "master",
  "excludePrivate": true,
  "excludeNotExported": true,
  "excludeProtected": true,
  "hideGenerator": true,
  "target": "ES6",
  "moduleResolution": "node",
  "preserveConstEnums": true,
  "stripInternal": true,
  "module": "commonjs"
}
